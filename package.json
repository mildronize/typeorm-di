{
  "name": "typeorm-di",
  "version": "0.0.4",
  "description": "It's just TypeORM, TypeDI and typeorm-typedi-extensions wrapper.",
  "author": "Thada Wangthammang",
  "repository": "mildronize/typeorm-di",
  "main": "dist/index.js",
  "license": "MIT",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
